<div class="container container1">
  <div class="row">
    <div class="col">
      <h1 class="headerText text-center">
        farmShare
      </h1>
    </div>
  </div>
  <div class="row">
    <div class="col">
      <h5 class="subHeader text-center">
        Add your tree to the schedule
      </h5>
    </div>
  </div>
  <div class="container-fluid container2">
    <div class="row justify-content-between">
      <div class="col-4">
        <div class="input-group mb-3">
          <input id="quantity" type="number" class="form-control" placeholder="Qty" aria-label="Username"
            aria-describedby="basic-addon1">
        </div>
      </div>
      <div class="col-6">
        <input id="title" type="text" class="form-control" placeholder="Type of Tree" aria-label="Username"
          aria-describedby="basic-addon1">
      </div>
    </div>
    <div class="row">
      <div class="col">
        <div class="input-group mb-3">
          <input id="address" type="text" class="form-control" placeholder="Address" aria-label="Username"
            aria-describedby="basic-addon1">
        </div>
      </div>
    </div>
    <div class="row justify-content-between">
      <div class="col-4">
        <input id="city" type="text" class="form-control" placeholder="City" aria-label="Username"
          aria-describedby="basic-addon1">
      </div>
      <div class="col-4">
        <div class="dropdown">
          <button id="dDownBtn" class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenu2"
            data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">

          </button>
          <div id="dDownMenu" class="dropdown-menu" aria-labelledby="dropdownMenu2">

          </div>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-4">
        <input id="date" type="date" class="form-control" placeholder="" aria-label="Username"
          aria-describedby="basic-addon1">
        <p class="text-center">Date </p>
      </div>
      <div class="col-4">
        <input id="start-time" type="time" class="form-control" placeholder="" aria-label="Username"
          aria-describedby="basic-addon1">
        <p class="text-center">Start Time</p>
      </div>
      <div class="col-4">
        <input id="end-time" type="time" class="form-control" placeholder="" aria-label="Username"
          aria-describedby="basic-addon1">
        <p class="text-center">End Time</p>
      </div>
    </div>
    <div class="row">
      <div class="col">
        <div class="input-group mb-3">
          <input id="instructions" type="text" class="form-control" placeholder="" aria-label="Username"
            aria-describedby="basic-addon1">
        </div>
        <p class="text-left">Instructions for Harvester</p>
      </div>
    </div>
    <div class="row justify-content-end">
      <div class="col-4">
        <button id="submit-btn" type="button" class="registerButton text-center btn btn-link">Submit</button>
      </div>
    </div>
  </div>
</div>
<div class="row footer">
  <div class="col-12">
    <p class="text-center">Copywrite 2019</a></p>
  </div>
</div>
</div>



